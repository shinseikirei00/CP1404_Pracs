import math
from sys import stdin


# Jan Crooks

def main():
    # t = int(input("Enter value for t: "))
    # a = int(input("Enter value for a: "))
    # b = int(input("Enter value for b: "))
    temp = stdin.readline()
    temp1 = temp.split()
    t = int(temp1[0].strip())
    a = int(temp1[1].strip())
    b = int(temp1[2].strip())

    for counter in range(0, t):
        sum = 0
        temp3 = stdin.readline()
        d = int(temp3.strip())
        c = a
        factors = []
        counter = 0
        for i in range(c, b + 1, ):
            factors = list(divisorGenerator(c))
            print(factors)
            for factor in factors:
                if factor % d == 1:
                    sum += 1
            c += 1
        print(sum)


def divisorGenerator(n):
    large_divisors = []
    for k in range(1, int(math.sqrt(n) + 1)):
        if n % k == 0:
            yield k
            if k * k != n:
                large_divisors.append(n / k)
    for divisor in reversed(large_divisors):
        yield divisor


main()